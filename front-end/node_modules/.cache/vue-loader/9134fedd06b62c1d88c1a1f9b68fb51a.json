rcaseUrlifiedUsername = usernameUrlified.toLowerCase();\n\n                this.authenticateHrExpert(lowercaseUrlifiedUsername, password).then(authenticationStatus => {\n                    console.log(authenticationStatus);\n                });\n            }\n        }\n    }\n</script>\n\n<style scoped lang=\"scss\">\n    @import '../../styles/abstracts/typography';\n    @import '../../styles/abstracts/colors';\n\n    .login-form {\n        width: 20rem;\n\n        label {\n            color: $slightly-blue-white;\n            font-size: 18px;\n        }\n\n        .text-muted {\n            color: $slightly-blue-white !important;\n        }\n    }\n\n    .hr-login {\n        background-image: linear-gradient(\n                to right bottom,\n                rgba(49, 147, 213, 0.42),\n                rgba(50, 134, 213, 0.33)),\n        url(../../assets/images/obss-banner.jpg);\n        background-size: cover;\n        background-position: top;\n        height: 100vh;\n        width: 100vw;\n        position: relative;\n        margin-bottom: 5rem;\n        @include set_title_font;\n    }\n</style>\n"]}]}