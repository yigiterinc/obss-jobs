le":"TalentScout.vue","sourceRoot":"src/components/HR-Admin","sourcesContent":["<template>\n<div>\n    <Navbar></Navbar>\n    <div class=\"container\">\n        <div class=\"form-group search\">\n            <label for=\"search\">Search anything</label>\n            <div class=\"form-inline\">\n                <input type=\"text\" class=\"form-control\" id=\"search\" placeholder=\"Search\" v-model=\"query\" required>\n                <button class=\"btn btn-info\" @click=\"search()\">Search</button>\n            </div>\n        </div>\n\n        <div class=\"card results-table\">\n            <table class=\"table table-hover\">\n                <thead class=\"thead-light\">\n                <tr>\n                    <th scope=\"col\">#</th>\n                    <th scope=\"col\">Name</th>\n                    <th scope=\"col\">Email</th>\n                    <th scope=\"col\">See skills</th>\n                </tr>\n                </thead>\n                <tbody>\n                <tr v-for=\"(searchResult, i) in searchResults\">\n                    <th scope=\"row\">{{ i + 1 }}</th>\n                    <td scope=\"row\">{{ searchResult.fullName }}</td>\n                    <td>{{ searchResult.email }}</td>\n                    <td>\n                        <button type=\"button\" class=\"btn btn-light\">See Skills</button>\n                    </td>\n                </tr>\n                </tbody>\n            </table>\n        </div>\n    </div>\n</div>\n</template>\n\n<script>\n    import Navbar from \"../Nav.vue\"\n    import ServicesMixin from \"../mixins/services-mixin\";\n\n    export default {\n        name: \"TalentScout\",\n        mixins: [ServicesMixin],\n        components: {\n            Navbar\n        },\n        data () {\n            return {\n                searchResults: [],\n                usersFound: {},\n                query: \"\"\n            }\n        },\n        methods: {\n            search: function () {\n                this.performFreeTextSearch(this.query).then(response => {\n                    this.searchResults = [];\n                    this.usersFound = [];\n                    console.log(response)\n\n                    response.forEach(entry => {\n                        let user = this.usersFound[this.getKey(entry)];\n                        console.log(user)\n\n                        if (!user) {\n                            console.log('user not found');\n                            this.usersFound[entry.fullName] = {\n                                candidateId: entry.candidateId,\n                                email: entry.email,\n                                skills: new Set(),\n                                blacklisted: entry.blacklisted\n                            };\n                            this.usersFound[entry.fullName].skills.add(entry.skill);\n                        } else {\n                            console.log('user found');\n                            this.usersFound[entry.fullName].skills.add(entry.skill);\n\n                            if (entry.blacklisted)\n                                this.usersFound[entry.fullName].blacklisted = true;\n                        }\n                    });\n\n                    console.log(this.usersFound);\n\n                    Object.keys(this.usersFound).forEach(key => {\n                        this.searchResults.push({\n                            candidateId: this.usersFound[key].candidateId,\n                            email: this.usersFound[key].email,\n                            fullName: key,\n                            skills: this.usersFound[key].skills\n                        })\n                    });\n\n                    console.log(this.searchResults)\n                })\n            },\n            getRedirectUrl: function (candidateId) {\n                this.$router.push(\"profile/\" + candidateId)\n            },\n            getKey: function(user) {\n                return user.fullName;\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n    #search {\n        width: 500px;\n        margin-right: 1rem;\n    }\n\n    .form-group {\n        margin-left: 15vw;\n    }\n\n    .table-hover {\n        margin-bottom: 0;\n    }\n\n    .btn {\n        background-color: darken(#e9ecef, 5%);\n    }\n\n    .results-table {\n        margin-top: 15vh;\n    }\n\n    .search {\n        margin-top: 5vh;\n    }\n</style>\n"]}]}