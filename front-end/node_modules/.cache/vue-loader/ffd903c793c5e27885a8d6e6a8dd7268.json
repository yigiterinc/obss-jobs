"version":3,"sources":["JobPosts.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;AAeA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"JobPosts.vue","sourceRoot":"src/components/Job-Posts-Page","sourcesContent":["<template>\n<div class=\"job-posts\" v-if=\"dataFetched\">\n    <Nav :is-candidate=\"true\" :has-logged-in=\"true\"></Nav>\n    <div class=\"container\">\n        <b-row class=\"row-container\" v-for=\"row in NUMBER_OF_ROWS\">\n            <b-col v-for=\"col in NUMBER_OF_COLS\">\n                <job-card class=\"j-card\" :job-post=\"jobPosts[getJobPostIndex(row, col)]\"></job-card>\n            </b-col>\n        </b-row>\n    </div>\n    <br><br>\n</div>\n</template>\n\n<script>\n    import Nav from \"../Nav.vue\";\n    import JobCard from \"./JobCard\";\n    import ServicesMixin from \"../mixins/services-mixin\";\n\n    export default {\n        name: \"JobPosts\",\n        components: {JobCard, Nav},\n        mixins: [ServicesMixin],\n        data () {\n            return {\n                jobPosts: null,\n                NUMBER_OF_COLS: 3,\n                NUMBER_OF_ROWS: 1,\n                dataFetched: false\n            }\n        },\n        methods: {\n            getJobPostIndex: function (row, col) {\n                return (row - 1) * 3 + (col - 1);\n            }\n        },\n        mounted() {\n            this.getJobPosts().then(jobPosts => {\n                this.jobPosts = jobPosts;\n                this.dataFetched = true;\n                this.NUMBER_OF_ROWS = Math.ceil(jobPosts.length / 3);\n                console.log(jobPosts);\n            }).catch(() => {\n                console.log(\"An error occurred while getting job posts\");\n            });\n        }\n    }\n</script>\n\n<style lang=\"scss\">\n@import \"../../styles/abstracts/colors\";\n@import \"../../styles/abstracts/typography\";\n\n.row-container {\n    display: flex;\n}\n\n.j-card {\n    height: 100%;\n}\n</style>\n\n"]}]}