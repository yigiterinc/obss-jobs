: function userAuthenticated() {\n      return this.$store.getters.userAuthenticated;\n    }\n  },\n  methods: {\n    isInAuthenticationProcess: function isInAuthenticationProcess() {\n      var url = window.location.search; // return the first url param\n\n      if (url && url.split(\"?\")) return url.split(\"?\")[1].substr(0, 4) === 'code';\n      return false;\n    },\n    exchangeLinkedInToken: function exchangeLinkedInToken(code) {\n      var endpoint = 'http://localhost:8888/api/services/controller' + '/linked-in' + '/exchange-token' + '?code=' + code;\n      return new Promise(function (resolve, reject) {\n        Vue.axios.post(endpoint).then(function (response) {\n          resolve(response);\n        }).catch(function (error) {\n          reject(error);\n        });\n      });\n    }\n  },\n  mounted: function mounted() {\n    var _this = this;\n\n    if (this.isInAuthenticationProcess() && !this.userAuthenticated) {\n      this.loginCompleted = true;\n      var url = window.location.search;\n      var split = url.split('?code=')[1];\n      var code = split.split('&state')[0];\n      this.exchangeLinkedInToken(code).then(function (response) {\n        _this.$store.commit('setUserId', response.data);\n\n        _this.loginCompleted = false;\n\n        _this.$swal(\"Login successful\", \"You successfully logged in, adding skills to your profile will \" + \"greatly increase your chances\", \"success\");\n      }).catch(function (error) {\n        if (error.response && error.response.status === 409) {\n          window.location.href = _this.linkedInLogin;\n          return;\n        }\n\n        console.error(error);\n      });\n    }\n  }\n};",{"version":3,"sources":["Home.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqCA,OAAA,GAAA;AACA,OAAA,WAAA;AAEA,OAAA,GAAA,MAAA,KAAA;AACA,OAAA,aAAA;AAEA,OAAA,OAAA,MAAA,qBAAA;AACA,OAAA,0CAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,MADA;AAEA,EAAA,MAAA,EAAA,CAAA,eAAA,CAFA;AAGA,EAAA,UAAA,EAAA;AACA,IAAA,WAAA,EAAA,W