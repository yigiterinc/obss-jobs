aveApplication: function saveApplication() {\n      if (this.userLoggedIn) {// check if the user has not applied to this job before\n        // send application to backend and save to db\n        // inform the user, say his application is taken and redirecting to jobs, wait 3s before redirecting\n      }\n    },\n    redirectBasedOnLoginStatus: function redirectBasedOnLoginStatus() {\n      // check if the user has logged in, if not redirect to login page\n      if (this.userLoggedIn) {\n        return \"/job-posts\";\n      } else {\n        return \"/linked-in-login\";\n      }\n    }\n  },\n  mounted: function mounted() {\n    var _this = this;\n\n    var jobId = this.getCurrentId();\n    this.getJobDetails(jobId).then(function (jobDetails) {\n      _this.jobDetails = jobDetails;\n    });\n    this.getJobRequirements(jobId).then(function (jobRequirements) {\n      jobRequirements.forEach(function (jobRequirementsDto) {\n        _this.jobRequirements.push(jobRequirementsDto.requirement);\n      });\n    });\n    this.dataFetched = true;\n  }\n};",{"version":3,"sources":["JobDetails.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6BA,OAAA,GAAA;AACA,OAAA,aAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,UADA;AAEA,EAAA,MAAA,EAAA,CAAA,aAAA,CAFA;AAGA,EAAA,UAAA,EAAA;AACA,IAAA,GAAA,EAAA;AADA,GAHA;AAMA,EAAA,IANA,kBAMA;AACA,WAAA;AACA,MAAA,UAAA,EAAA,EADA;AAEA,MAAA,eAAA,EAAA,EAFA;AAGA,MAAA,WAAA,EAAA,KAHA;AAIA,MAAA,YAAA,EAAA;AAJA,KAAA;AAMA,GAbA;AAcA,EAAA,OAAA,EAAA;AACA,IAAA,YAAA,EAAA,wBAAA;AACA,UAAA,GAAA,GAAA,MAAA,CAAA,QAAA,CAAA,QAAA,CADA,CAEA;;AACA,aAAA,GAAA,CAAA,KAAA,CAAA,GAAA,EAAA,CAAA,CAAA;AACA,KALA;AAMA,IAAA,eAAA,EAAA,2BAAA;AACA,UAAA,KAAA,YAAA,EAAA,CACA;AACA;AACA;AACA;AACA,KAZA;AAaA,IAAA,0BAAA,EAAA,sCAAA;AACA;AACA,UAAA,KAAA,YAAA,EAAA;AACA,eAAA,YAAA;AACA,OAFA,MAEA;AACA,eAAA,kBAAA;AACA;AACA;AApBA,GAdA;AAoCA,EAAA,OApCA,qBAoCA;AAAA;;AACA,QAAA,KAAA,GAAA,KAAA,YAAA,EAAA;AAEA,SAAA,aAAA,CAAA,KAAA,EAAA,IAAA,CAAA,UAAA,UAAA,EAAA;AACA,MAAA,KAAA,CAAA,UAAA,GAAA,UAAA;AACA,KAFA;AAIA,SAAA,kBAAA,CAAA,KAAA,EAAA,IAAA,CAAA,UAAA,eAAA,EAAA;AAEA,MAAA,eAAA,CAAA,OAAA,CAAA,UAAA,kBAAA,EAAA;AACA,QAAA,KAAA,CAAA,eAAA,CAAA,IAAA,CAAA,kBAAA,CAAA,WAAA;AACA,OAFA;AAGA,KALA;AAOA,SAAA,WAAA,GAAA,IAAA;AACA;AAnDA,CAAA","sourcesContent":["<template>\n<div>\n    <Nav></Nav>\n    <br>\n    <div class=\"container post\" v-if=\"dataFetched\">\n        <h1 class=\"post__title\">{{jobDetails.title}}</h1>\n        <p class=\"post__description\">{{jobDetails.description}}</p>\n        <div class=\"post__requirements\">\n            <p class=\"post__requirements--invite\"><strong>If you think you satify these requirements:</strong></p>\n            <ul v-for=\"requirement in jobRequirements\">\n                <li>{{requirement}}</li>\n            </ul>\n            <div class=\"space\"></div>\n            <p><strong>We would like to see you in our team!</strong></p>\n        </div>\n        <div class=\"job-card__bottom-container\">\n            <b-button type=\"button\" class=\"btn-info post__buttons--apply-button\">\n                <router-link :onclick=\"saveApplication()\"\n                             :to=\"redirectBasedOnLoginStatus()\"\n                             class=\"link\">\n                    Apply\n                </router-link>\n            </b-button>\n        </div>\n    </div>\n</div>\n</template>\n\n<script>\n    import Nav from \"../Nav.vue\";\n    import ServicesMixin from \"../mixins/services-mixin\";\n\n    export default {\n        name: \"JobApply\",\n        mixins: [ServicesMixin],\n        components: {\n            Nav: Nav\n        },\n        data() {\n            return {\n                jobDetails: {},\n                jobRequirements: [],\n                dataFetched: false,\n                userLoggedIn: true\n            }\n        },\n        methods: {\n            getCurrentId: function() {\n                const url = window.location.pathname;\n                // return the first url param\n                return url.split(\"/\")[2];\n            },\n            saveApplication: function() {\n                if (this.userLoggedIn) {\n                    // check if the user has not applied to this job before\n                    // send application to backend and save to db\n                    // inform the user, say his application is taken and redirecting to jobs, wait 3s before redirecting\n                }\n            },\n            redirectBasedOnLoginStatus: function() {\n                // check if the user has logged in, if not redirect to login page\n                if (this.userLoggedIn) {\n                    return \"/job-posts\";\n                } else {\n                    return \"/linked-in-login\";\n                }\n            }\n        },\n        mounted() {\n            const jobId = this.getCurrentId();\n\n            this.getJobDetails(jobId).then(jobDetails => {\n                this.jobDetails = jobDetails;\n            });\n\n            this.getJobRequirements(jobId).then(jobRequirements => {\n\n                jobRequirements.forEach(jobRequirementsDto => {\n                    this.jobRequirements.push(jobRequirementsDto.requirement);\n                })\n            });\n\n            this.dataFetched = true;\n        }\n    }\n</script>\n\n<style scoped lang=\"scss\">\n    @import '../../styles/abstracts/colors';\n    @import '../../styles/abstracts/typography';\n\n    .post {\n        @include set_font;\n        border: 3px solid $color-primary-0;\n        border-radius: 40px;\n        padding: 2rem 2rem 0 2rem;\n    }\n\n    .post__title {\n        font-size: 2.5rem;\n        font-weight: lighter;\n        margin-bottom: 2rem;\n        text-align: center;\n    }\n\n    .post__description {\n        margin-bottom: 2rem;\n    }\n\n    .post__requirements {\n        margin-left: 1rem;\n    }\n\n    .job-card__bottom-container {\n        height: 5rem;\n        width: 75vw;\n        position: relative;\n    }\n\n    .post__buttons--apply-button {\n        position: absolute;\n        top: 30%;\n        right: 1rem;\n        padding: 10px 20px 10px 20px;\n        border-radius: 200px;\n\n        &:hover, &:active {\n            background: $color-primary-0;\n            transform: scale(1.1) perspective(1px)\n        }\n\n    }\n\n    .btn-info {\n        background: $color-primary-2 !important;\n    }\n\n    .link {\n        @include remove_link_styles;\n    }\n\n    li {\n        list-style: none;\n        border-bottom: 1px solid #dddddd;\n        padding-top: 1.5rem;\n        margin-top: 0;\n\n        &:hover {\n            background: rgba($color-complement-2, 40%);\n        }\n    }\n\n    .space {\n        height: 2rem;\n    }\n\n    ul {\n        margin-bottom: 0;\n    }\n\n    .post__requirements--invite {\n        margin-bottom: 1rem;\n    }\n</style>\n"],"sourceRoot":"src/components/JobDetailsPage"}]}